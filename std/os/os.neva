#extern(args)
pub def Args(sig any) (data list<string>)

// Environ retrieves the current process environment variables in KEY=VALUE
// form. It returns a list of strings (not a stream) because the runtime
// function produces a full list at once. If you need only specific values,
// filter the returned list in your flow.
#extern(os_environ)
pub def Environ(sig any) (res list<string>)
