import { io, fmt, runtime, bytes }

def Main(start any) (stop any) {
	readAll io.ReadAll
	toString bytes.ToString
	println fmt.Println
	panic runtime.Panic
	---
	:start -> 'main.neva' -> readAll:filename
	readAll:res -> toString -> println:data
	println:res -> :stop
	[readAll:err, println:err] -> panic
}
