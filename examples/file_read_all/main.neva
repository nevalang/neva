import {
	bytes
	fmt
	io
	runtime
	strings
}

def Main(start any) (stop any) {
	read_all io.ReadAll
	string_from_bytes strings.FromBytes
	println fmt.Println
	panic runtime.Panic
	---
	:start -> 'main.neva' -> read_all:filename
	read_all:res -> string_from_bytes -> println:data
	println:res -> :stop
	[read_all:err, println:err] -> panic
}
