import { lists, fmt, runtime, streams }

const lst list<int> = [50, 30, 20, 100]

def Main(start any) (stop any) {
    s2l lists.FromStream<int>
    map_dec streams.Map<int, int>{Dec}
    println fmt.Println<list<int>>
    l2s streams.FromList<int>
    panic runtime.Panic
    ---
    :start -> $lst -> l2s -> map_dec -> s2l -> println:data
    println:res -> :stop
    println:err -> panic
}
