import {
    fmt
    runtime
    streams
}

const lst list<int> = [1, 2, 3]

def Main(start any) (stop any) {
    streams.FromList<int>, streams.ForEach<int>{fmt.Println<int>}, streams.Wait, runtime.Panic
    ---
    :start -> $lst -> listToStream -> for
    for:res -> wait -> :stop
    for:err -> panic
}
