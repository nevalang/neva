import {
    fmt
    runtime
    streams
}

const lst list<int> = [1, 2, 3]

def Main(start any) (stop any) {
    list_to_stream streams.FromList<int>
    for_each streams.ForEach<int>{fmt.Println<int>}
    wait streams.Wait
    panic runtime.Panic
    ---
    :start -> $lst -> list_to_stream -> for_each
    for_each:res -> wait -> :stop
    for_each:err -> panic
}
