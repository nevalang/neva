import { streams, fmt, runtime }

const strings list<string> = ['a', 'b', 'c']

def Main(start any) (stop any) {
	ListToStream<string>, streams.Zip<int, string>
	range streams.Range
	For{fmt.Println}, streams.Wait, runtime.Panic
	---
	:start -> [
		0 -> range:from,
		10 -> range:to
	]
	range -> zip:left
	$strings -> listToStream -> zip:right
	zip:res -> for
	for:res -> wait -> :stop
	for:err -> panic
}
