import { streams, fmt, runtime }

const strings list<string> = ['a', 'b', 'c']

def Main(start any) (stop any) {
        streams.FromList<string>, streams.Zip<int, string>
        streams.ForEach{fmt.Println}, streams.Wait, runtime.Panic
	---
        :start -> 0..10 -> zip:left
        $strings -> listToStream -> zip:right
        zip:res -> for
	for:res -> wait -> :stop
	for:err -> panic
}
