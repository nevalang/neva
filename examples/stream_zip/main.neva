import {
	fmt
	runtime
	streams
}

const strings list<string> = ['a', 'b', 'c']

def Main(start any) (stop any) {
	list_to_stream ListToStream<string>
	zip streams.Zip<int, string>
	for_println For{fmt.Println}
	wait streams.Wait
	panic runtime.Panic
	---
	:start -> [
		0..10 -> zip:left,
		$strings -> list_to_stream -> zip:right
	]
	zip:res -> for_println
	for_println:res -> wait -> :stop
	for_println:err -> panic
}
