import { io, fmt, runtime, bytes }

def Main(start any) (stop any) {
	writeAll io.WriteAll
	fromString bytes.FromString
	println fmt.Println
	panic runtime.Panic
	---
	:start -> [
		'file_writer_example.txt' -> writeAll:filename,
		'Hello, io.WriteAll!' -> fromString
	]
	fromString:res -> writeAll:data
	writeAll:err -> println
	[writeAll:res, println:res] -> :stop
	println:err -> panic
}
