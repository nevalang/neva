import { streams, fmt, runtime }

type ProductResult streams.ProductResult<int, int>

def Main(start any) (stop any) {
        streams.Product<int, int>
        streams.ForEach<ProductResult>{fmt.Println<ProductResult>}
        streams.Wait
	runtime.Panic
	---
	:start -> [
		0..3 -> product:first,
		0..3 -> product:second
	]
	product -> for
	for:res -> wait -> :stop
	for:err -> panic
}
