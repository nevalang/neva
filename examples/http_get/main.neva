import {
	fmt
	http
	runtime
	strings
}

def Main(start any) (stop any) {
	http_get http.Get
	string_from_bytes strings.FromBytes
	println fmt.Println
	panic runtime.Panic
	---
	:start -> 'http://www.example.com' -> http_get
	http_get:res -> .body -> string_from_bytes -> println
	println:res -> :stop
	[http_get:err, println:err] -> panic
}
