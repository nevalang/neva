interface IPrinter<T>(data T) (sig T)

component {
    Main(start any) (stop any) {
        nodes {
            SecondComponent {
                depNode Println<any>
            }
        }
        net {
            :start -> secondComponent:msg
            secondComponent:msg -> :stop
        }
    }
    
    SecondComponent (msg any) (msg any) {
        nodes { depNode IPrinter<any> }
        net {
            :msg -> depNode:data
            depNode:sig -> :msg
        }
    }
}