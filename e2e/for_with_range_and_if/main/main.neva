import { lists, fmt }

const lst list<bool> = [true, false]

def Main(start any) (stop any) {
	ListToStream<bool>, For<bool>{PrintAsNum}, Wait, Panic
	---
	:start -> $lst -> listToStream -> for
	for:res -> wait -> :stop
	for:err -> panic
}

def PrintAsNum(data bool) (sig any) {
	Ternary, fmt.Println
	---
	:data -> ternary:if
	1 -> ternary:then
	0 -> ternary:else
	ternary -> println -> :sig
}
