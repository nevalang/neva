import {
    fmt
    runtime
}

type IntOrString union {
    Int int
    String string
}

def Main(start any) (stop any) {
    switch Switch<int>
    println fmt.Println<int>
    panic runtime.Panic
    ---
    :start -> 1 -> switch:data
    IntOrString::Int -> switch:case[0] -> println:data
    [switch:else, println:err] -> panic
    println:res -> :stop
}
