import {
    fmt
    runtime
    streams
}

const lst list<int> = [50, 30, 20, 100]

def Main(start any) (stop any) {
    streams.FromList<int>, streams.ForEach<int>{fmt.Println}, streams.Wait, runtime.Panic
    ---
    :start -> $lst -> listToStream -> for
    for:res -> wait -> :stop
    for:err -> panic
}
