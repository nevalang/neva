import {
    fmt
    runtime
    streams
}

const lst list<int> = [50, 30, 20, 100]

def Main(start any) (stop any) {
    from_list streams.FromList<int>
    for_each streams.ForEach<int>{fmt.Println<int>}
    wait streams.Wait
    panic runtime.Panic
    ---
    :start -> $lst -> from_list -> for_each
    for_each:res -> wait -> :stop
    for_each:err -> panic
}
