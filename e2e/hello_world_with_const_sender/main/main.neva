import {
    fmt
    runtime
}

const greeting string = 'Hello, World!'

def Main(start any) (stop any) {
        println fmt.Println<string>
        lock Lock<string>
        panic runtime.Panic
        ---
        :start -> [
            lock:sig,
            $greeting -> lock:data
        ]
        lock:data -> println:data
        println:res -> :stop
        println:err -> panic
}